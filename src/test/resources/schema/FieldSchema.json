{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "_value"
  ],
  "properties": {
    "_value": {
      "$id": "#/properties/_value",
      "type": "string",
      "title": "The _value Schema",
      "default": "",
      "examples": [
        "Grand Canyon National Park"
      ],
      "pattern": "^(.*)$"
    }
  }
}
