{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "name",
    "closed",
    "idBoard",
    "pos"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "5d5dae5f188e7f134d9c49b2"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "List4"
      ],
      "pattern": "^(.*)$"
    },
    "closed": {
      "$id": "#/properties/closed",
      "type": "boolean",
      "title": "The Closed Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "idBoard": {
      "$id": "#/properties/idBoard",
      "type": "string",
      "title": "The Idboard Schema",
      "default": "",
      "examples": [
        "5d5dae372fac806ba4e591cd"
      ],
      "pattern": "^(.*)$"
    },
    "pos": {
      "$id": "#/properties/pos",
      "type": "integer",
      "title": "The Pos Schema",
      "default": 0,
      "examples": [
        16384
      ]
    }
  }
}
